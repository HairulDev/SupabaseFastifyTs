{
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node"
        },
        {
            "src": "src/app.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/app.ts"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/src/app.ts"
        }
    ]
}